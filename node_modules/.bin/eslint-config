#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@antfu+eslint-config@3.16.0_932a03af96b50dadea0f18e4add6cfc0/node_modules/@antfu/eslint-config/bin/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@antfu+eslint-config@3.16.0_932a03af96b50dadea0f18e4add6cfc0/node_modules/@antfu/eslint-config/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@antfu+eslint-config@3.16.0_932a03af96b50dadea0f18e4add6cfc0/node_modules/@antfu/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@antfu+eslint-config@3.16.0_932a03af96b50dadea0f18e4add6cfc0/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@antfu+eslint-config@3.16.0_932a03af96b50dadea0f18e4add6cfc0/node_modules/@antfu/eslint-config/bin/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@antfu+eslint-config@3.16.0_932a03af96b50dadea0f18e4add6cfc0/node_modules/@antfu/eslint-config/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@antfu+eslint-config@3.16.0_932a03af96b50dadea0f18e4add6cfc0/node_modules/@antfu/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@antfu+eslint-config@3.16.0_932a03af96b50dadea0f18e4add6cfc0/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@antfu+eslint-config@3.16.0_932a03af96b50dadea0f18e4add6cfc0/node_modules/@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/@antfu+eslint-config@3.16.0_932a03af96b50dadea0f18e4add6cfc0/node_modules/@antfu/eslint-config/bin/index.js" "$@"
fi
