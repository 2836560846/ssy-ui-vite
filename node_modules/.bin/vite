#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_4b780a19fc0db12823351d40f32901ff/node_modules/vite/bin/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_4b780a19fc0db12823351d40f32901ff/node_modules/vite/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_4b780a19fc0db12823351d40f32901ff/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_4b780a19fc0db12823351d40f32901ff/node_modules/vite/bin/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_4b780a19fc0db12823351d40f32901ff/node_modules/vite/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_4b780a19fc0db12823351d40f32901ff/node_modules:/mnt/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@7.1.9_@types+node@24.7_4b780a19fc0db12823351d40f32901ff/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@7.1.9_@types+node@24.7_4b780a19fc0db12823351d40f32901ff/node_modules/vite/bin/vite.js" "$@"
fi
