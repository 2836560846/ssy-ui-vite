#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vitepress-theme-demoblock@3_b8a29b69a7e96238d90e668dccd19c85/node_modules/vitepress-theme-demoblock/bin/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vitepress-theme-demoblock@3_b8a29b69a7e96238d90e668dccd19c85/node_modules/vitepress-theme-demoblock/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vitepress-theme-demoblock@3_b8a29b69a7e96238d90e668dccd19c85/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vitepress-theme-demoblock@3_b8a29b69a7e96238d90e668dccd19c85/node_modules/vitepress-theme-demoblock/bin/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vitepress-theme-demoblock@3_b8a29b69a7e96238d90e668dccd19c85/node_modules/vitepress-theme-demoblock/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/vitepress-theme-demoblock@3_b8a29b69a7e96238d90e668dccd19c85/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress-register-components.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress-register-components.js" "$@"
fi
