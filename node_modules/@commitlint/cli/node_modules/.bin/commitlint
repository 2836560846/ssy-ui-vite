#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@commitlint+cli@20.1.0_@types+node@24.7.0_typescript@5.5.4/node_modules/@commitlint/cli/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@commitlint+cli@20.1.0_@types+node@24.7.0_typescript@5.5.4/node_modules/@commitlint/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@commitlint+cli@20.1.0_@types+node@24.7.0_typescript@5.5.4/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@commitlint+cli@20.1.0_@types+node@24.7.0_typescript@5.5.4/node_modules/@commitlint/cli/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@commitlint+cli@20.1.0_@types+node@24.7.0_typescript@5.5.4/node_modules/@commitlint/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/@commitlint+cli@20.1.0_@types+node@24.7.0_typescript@5.5.4/node_modules:/proc/cygdrive/d/作业/web前端开发/ssy-ui-vite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
